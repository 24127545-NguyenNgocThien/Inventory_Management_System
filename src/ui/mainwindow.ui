             <widget class="QDateEdit" name="dateFrom">
              <property name="styleSheet">
               <string notr="true">QDateEdit, QDateTimeEdit, QSpinBox {
    border: 1px solid #1e3a8a;
    border-radius: 6px;
    padding: 2px 24px 2px 8px;   /* chừa chỗ bên phải cho nút mũi tên */
    background: #ffffff;
    color: #111827;
    font: 10pt &quot;Segoe UI&quot;;
    min-height: 26px;
}

/* Hover */
QDateEdit:hover, QDateTimeEdit:hover, QSpinBox:hover {
    border: 1px solid #2563eb;
}

/* Focus */
QDateEdit:focus, QDateTimeEdit:focus, QSpinBox:focus {
    border: 1px solid #2563eb;
    background: #eff6ff;
    outline: none;
}

/* Up/Down button */
QSpinBox::up-button, QDateEdit::up-button, QDateTimeEdit::up-button,
QSpinBox::down-button, QDateEdit::down-button, QDateTimeEdit::down-button {
    subcontrol-origin: border;
    width: 20px;
    border: none;
    background: transparent;
}

/* Hover effect cho button */
QSpinBox::up-button:hover, QDateEdit::up-button:hover, QDateTimeEdit::up-button:hover,
QSpinBox::down-button:hover, QDateEdit::down-button:hover, QDateTimeEdit::down-button:hover {
    background: #e0e7ff;
    border-radius: 3px;
}

/* Up Arrow */
QSpinBox::up-arrow, QDateEdit::up-arrow, QDateTimeEdit::up-arrow {
    width: 8px;
    height: 8px;
}

/* Down Arrow */
QSpinBox::down-arrow, QDateEdit::down-arrow, QDateTimeEdit::down-arrow {
 image: none;
    width: 8px;
    height: 8px;
} 
</string>
